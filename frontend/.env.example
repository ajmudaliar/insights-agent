# Botpress Configuration
# Get your PAT from: https://app.botpress.cloud/settings/access-tokens
VITE_BOTPRESS_PAT=bp_pat_...

# Your Insights Agent bot details
VITE_BOTPRESS_WORKSPACE_ID=your_workspace_id
VITE_BOTPRESS_BOT_ID=your_bot_id

# Target bot configuration (the bot being analyzed)
# These should point to the bot whose conversations you want to analyze
VITE_TARGET_BOT_WORKSPACE_ID=your_target_workspace_id
VITE_TARGET_BOT_ID=your_target_bot_id
